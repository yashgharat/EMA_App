<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.Earnings.pastEarningsActivity">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/journalHistoryPrevious"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/ic_arrow_back_blue_30dp"
        android:scaleX="1"
        android:scaleY="1"
        app:backgroundTint="@color/unapparent"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <TextView
        android:id="@+id/journal_history_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="All Journal Entries"
        android:textSize="34sp"
        android:textStyle="bold"
        android:textColor="@color/prominent"
        app:layout_constraintTop_toBottomOf="@id/journalHistoryPrevious"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="37dp"
        android:layout_marginLeft="30dp"
        android:paddingBottom="30dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>